<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oyasumisoft.juanfrancrater.ipewa">

    <application
        android:name=".util.ThisApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.Splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.login.View.LoginActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ui.Welcome.WelcomeActivity" />
        <activity android:name=".ui.about.AboutActivity" />
        <activity android:name=".ui.task.View.ListTaskActivity" />
        <activity android:name=".ui.project.View.ListProjectActivity" />
        <activity android:name=".ui.pref.PrefferencesActivity" />
        <activity android:name=".ui.project.View.AddProjectActivity" />
        <activity android:name=".ui.project.View.EditProjectActivity" />
        <activity android:name=".ui.task.TabListTab"/>
        <activity
            android:name=".ui.task.View.AddTaskActivity"
            android:label="@string/title_activity_add_task"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.task.View.EditTaskActivity"
            android:label="@string/title_activity_edit_task"
            android:theme="@style/AppTheme" />
        <activity android:name=".ui.Register.View.RegisterActivity" />
        <activity android:name=".ui.project.View.DetailProjectActivity"></activity>
    </application>

</manifest>